{
  "data_graph": [
    {
      "selector": "node",
      "style": {
        "label": "data(label)",
        "shape": "round-rectangle",
        "color": "#000000",
        "background-color": "#ffffff",
        "border-color": "#333333",
        "border-width": 2,
        "text-valign": "center",
        "text-halign": "center",
        "width": 80,
        "height": 50,
        "font-weight": "bold",
        "font-size": 14,
        "text-wrap": "wrap",
        "text-max-width": 70
      }
    },
    {
      "selector": "node[color]",
      "style": {
        "background-color": "data(color)"
      }
    },
    {
      "selector": "edge",
      "style": {
        "label": "data(weight)",
        "width": 2,
        "line-color": "#888888",
        "target-arrow-color": "#888888",
        "target-arrow-shape": "triangle",
        "curve-style": "bezier",
        "font-size": 10,
        "text-background-opacity": 1,
        "text-background-color": "#ffffff"
      }
    },
    {
      "selector": ".current",
      "style": {
        "background-color": "#FFD700",
        "border-color": "#FF8C00",
        "border-width": 3
      }
    },
    {
      "selector": ".visited",
      "style": {
        "background-color": "#e6f7ff",
        "border-color": "#1890ff"
      }
    }
  ],
  "flowchart": [
    {
      "selector": "node",
      "style": {
        "label": "data(label)",
        "shape": "rectangle",
        "background-color": "#f0f2f6",
        "border-color": "#31333F",
        "border-width": 1,
        "width": 140,
        "height": 60,
        "text-valign": "center",
        "text-halign": "center",
        "text-wrap": "wrap",
        "text-max-width": 130,
        "font-size": 12
      }
    },
    {
      "selector": ".flow-decision",
      "style": {
        "shape": "diamond",
        "background-color": "#fff3cd",
        "border-color": "#856404",
        "width": 140,
        "height": 70
      }
    },
    {
      "selector": ".flow-start",
      "style": {
        "shape": "ellipse",
        "background-color": "#d4edda",
        "border-color": "#155724",
        "width": 100,
        "height": 50
      }
    },
    {
      "selector": ".flow-terminator",
      "style": {
        "shape": "round-rectangle",
        "background-color": "#f8d7da",
        "border-color": "#721c24",
        "width": 120,
        "height": 50
      }
    },
    {
      "selector": ".active-step",
      "style": {
        "background-color": "#FFD700",
        "border-color": "#FF0000",
        "border-width": 3,
        "font-weight": "bold"
      }
    }
  ]
}